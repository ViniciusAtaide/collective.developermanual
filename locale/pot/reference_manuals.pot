msgstr ""

#: ../../source/reference_manuals/external/plone.app.theming/userguide.rst:122
# 25fb91f08fcd43b98c749a6cdc1fa4d6
msgid "Creating a new theme"
msgstr ""

#: ../../source/reference_manuals/external/plone.app.theming/userguide.rst:124
# 25fcfbbbfe994fef82f61e262d12e304
msgid "New themes can be created in one of two ways:"
msgstr ""

