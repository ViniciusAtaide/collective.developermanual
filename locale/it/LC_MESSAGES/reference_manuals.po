msgstr ""

# 94160cdda0ca4269af7a57538969ab61
#: ../../source/reference_manuals/old/archetypes/appendix.rst:2150
msgid "Contains unit and integration tests."
msgstr ""

# 39d27b4e07284c7b8a7a4b57248a71df
#: ../../source/reference_manuals/old/archetypes/appendix.rst:2152
